{
  "script_id": "customer_roleplay_flow_v1",
  "version": 1,
  "roles": { "bot": "Customer", "user": "Agent" },
  "notes": {
    "purpose": "Customer-roleplay utterance library grouped by stage. Use stage policy + triggers to select utterances at runtime.",
    "timezone": "America/Los_Angeles"
  },
  "stages": [
    {
      "stage_id": 2,
      "name": "Motivation Discovery",
      "policy": {
        "min_utterances": 4,
        "max_utterances": 7,
        "required_tags": ["motivation", "timeline_or_urgency"],
        "next_stage": 3
      }
    },
    {
      "stage_id": 3,
      "name": "Qualification & Depth",
      "policy": {
        "min_utterances": 5,
        "max_utterances": 8,
        "required_tags": ["budget_or_affordability", "bedrooms_or_size", "constraints"],
        "next_stage": 4
      }
    },
    {
      "stage_id": 4,
      "name": "Objection Handling",
      "policy": {
        "min_utterances": 3,
        "max_utterances": 6,
        "required_tags": ["objection"],
        "next_stage": 5
      }
    },
    {
      "stage_id": 5,
      "name": "Value Frame Testing",
      "policy": {
        "min_utterances": 1,
        "max_utterances": 3,
        "required_tags": ["value_test"],
        "next_stage": 6
      }
    },
    {
      "stage_id": 6,
      "name": "Momentum & Next Stage",
      "policy": {
        "min_utterances": 2,
        "max_utterances": 4,
        "required_tags": ["next_step_resistance"],
        "next_stage": null
      }
    }
  ],
  "transitions": [
    {
      "from_stage": 2,
      "trigger": "agent_pushes_listings_early",
      "to_stage": 4,
      "note": "Escalate resistance if agent goes to listings/showings before discovery."
    },
    {
      "from_stage": 3,
      "trigger": "agent_pushes_offer_or_pressure",
      "to_stage": 4,
      "note": "If agent introduces pressure or offer talk prematurely, go to objections."
    }
  ],
  "classifiers": {
    "agent_acknowledged": {
      "type": "rules_or_llm",
      "description": "Agent validates/acknowledges the customer's stance and asks a discovery question without pressure."
    },
    "agent_pushes_listings_early": {
      "type": "rules_or_llm",
      "description": "Agent pushes listings/showings/appointments or uses urgency without discovery."
    },
    "agent_pushes_offer_or_pressure": {
      "type": "rules_or_llm",
      "description": "Agent pushes commitment/offer/representation agreement too early or uses fear/urgency framing."
    }
  },
  "variant_groups": [
    {
      "group_id": "housing_status",
      "note": "Pick one per session unless you intentionally want mixed signals.",
      "members": ["renting_now", "selling_current_home"]
    },
    {
      "group_id": "financing_status",
      "note": "Pick one per session unless testing contradictions.",
      "members": ["preapproved", "no_lender_yet", "not_preapproved_yet"]
    },
    {
      "group_id": "directional_need",
      "note": "Upsize vs downsize — usually choose one.",
      "members": ["upsizing", "downsizing"]
    }
  ],
